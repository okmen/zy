package com.egou.search.web;

@Controller
@RequestMapping(value = "/login")
public class SearchController {

	
	@ResponseBody
	@RequestMapping(value = "/productall",method ={RequestMethod.POST,RequestMethod.GET})
	public String findAllProduct() throws Exception {
		List<PProduct> list=productDao.findAll();
		return JsonUtils.objectToJson(list);
	}
}
